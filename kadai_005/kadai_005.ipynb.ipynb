        "# 台形の面積を求めるプログラム\n",
        "\n",
        "top = 10      # 上辺\n",
        "bottom = 20   # 下辺\n",
        "height = 5    # 高さ\n",
        "\n",
        "area = (top + bottom) * height / 2\n",
        "\n",
        "print(str(area) + \"cm²\")\n"
